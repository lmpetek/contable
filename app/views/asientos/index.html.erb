<h1>Listing asientos</h1>
<table class="tabla" width="100%">
  <thead>
    <tr>
      <th width="30%" align="center">Descripcion</th>
      <th width="20%" align="center">Fecha</th>
      <th width="10%" align="center">Ejercicio</th>
      <th width="10%" align="center"></th>
      <th width="10%" align="center"></th>
      <th width="10%" align="center"></th>
      <th width="10%" align="center"></th>
    </tr>
  </thead>
  <tbody>
  <% @asientos.each do |asiento| %>
  <tr>
    <td align="left"> <%= asiento.descripcion %> </td>
    <td align="center"> <%= asiento.fecha %> </td>
    <td align="right"> <%= asiento.ejercicio.descripcion unless asiento.ejercicio.nil? %> </td>
    <td align="right"> <%= link_to 'Show', asiento %></td>
    <td align="right"> <%= link_to 'Edit', edit_asiento_path(asiento) %></td>
    <td align="right"> <%= link_to 'Imprimir', asiento, :confirm => 'quiere imprimir?', :method => :print %></td>
    <td align="right"> <%= link_to 'Destroy', asiento, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
  <% end %>
  </tbody>
</table>
<br />
<%= link_to 'New Asiento', new_asiento_path %>
