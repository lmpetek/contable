<h1>Listing asientos</h1>

<table>
  <tr>
    <th>Descripcion</th>
    <th>Fecha</th>
    <th>Ejercicio</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @asientos.each do |asiento| %>
  <tr>
    <td><%= asiento.descripcion %></td>
    <td><%= asiento.fecha %></td>
    <td><%= asiento.ejercicio %></td>
    <td><%= link_to 'Show', asiento %></td>
    <td><%= link_to 'Edit', edit_asiento_path(asiento) %></td>
    <td><%= link_to 'Imprimir', asiento, :confirm => 'quiere imprimir?', :method => :print %></td>
    <td><%= link_to 'Destroy', asiento, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Asiento', new_asiento_path %>
